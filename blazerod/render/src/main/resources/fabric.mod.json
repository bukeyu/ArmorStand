{
  "schemaVersion": 1,
  "id": "blazerod-render",
  "version": "${version}",
  "name": "BlazeRod render",
  "description": "${description}",
  "authors": ["fifth_light"],
  "license": "LGPL-3.0-or-later",
  "environment": "client",
  "entrypoints": {
    "client": [
      {
        "value": "top.fifthlight.blazerod.BlazeRod",
        "adapter": "kotlin"
      }
    ]
  },
  "accessWidener": "blazerod.accesswidener",
  "mixins": [
    {
      "environment": "client",
      "config": "blazerod.mixins.json"
    }
  ],
  "depends": {
    "minecraft": "=1.21.7",
    "java": ">=21",
    "fabric-api": "*"
  },
  "custom": {
    "modmenu": {
      "parent": "blazerod",
      "badges": ["library"],
      "environment": "client"
    }
  }
}
